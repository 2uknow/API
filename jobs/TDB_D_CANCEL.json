{
  "name": "TDB_D_CANCEL",
  "type": "binary",
  "description": "Simple YAML-based SClient test for system validation with HTML report",
  "collection": "collections/Danal_Teledit_Cancel_D_v1.0/",
  "excludePatterns": [
    "test_config.yaml",
    "common.yaml", 
    "enhanced_chai_test.yaml",
    "*conf*.yaml"
  ],
  "environment": "Production",
  "platforms": {
    "win32": {
      "executable": "SClient.exe",
      "path": "binaries/windows"
    }
  },
  "timeout": 15000,
  "encoding": "cp949",
  "parseOutput": {
    "successPattern": "Result=0|SUCCESS",
    "failurePattern": "Result=[^0]|ERROR|ErrMsg=",
    "statsPattern": "Result=([0-9]+)"
  },
  "generateHtmlReport": true,
  "reportOptions": {
    "title": "Simple YAML Test Execution Report",
    "browserTitle": "Simple YAML Test Report"
  }
}