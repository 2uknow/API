{
  "name": "batch_test",
  "type": "binary",
  "description": "Execute all YAML files in collections directory as batch",
  "collection": "collections/",
  "environment": "Production",
  "excludePatterns": [
    "*config*",
    "_*",
    "*.config.yaml",
    "**/config/**",
    "*common*"
  ],
  "platforms": {
    "win32": {
      "executable": "SClient.exe",
      "path": "binaries/windows"
    }
  },
  "timeout": 60000,
  "encoding": "cp949",
  "parseOutput": {
    "successPattern": "Result=0|SUCCESS",
    "failurePattern": "Result=[^0]|ERROR|ErrMsg=",
    "statsPattern": "Result=([0-9]+)"
  },
  "generateHtmlReport": true,
  "reportOptions": {
    "title": "Batch YAML Test Execution Report",
    "browserTitle": "Batch YAML Test Report"
  }
}